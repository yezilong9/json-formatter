{
  "name": "json-formatter",
  "description": "一个轻量的json解释工具，支持在线图片直接浏览",
  "version": "0.3",
  "incognito": "split",
  "browser_action": {
    "default_icon": "static/img/icon.png"
  },
  "icons": {
    "16": "static/img/icon.png",
    "48": "static/img/icon.png",
    "128": "static/img/icon.png"
  },
  "background":{
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2
}